{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "self": "readonly"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off"
  }
}
